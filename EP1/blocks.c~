#include <stdio.h>
#include <stdlib.h>
#include <graph.h>

int* grau;
int* pai;

Graph G;

int numVertices;

int main(int argc, char* argv){
  
  while(scanf("%d",&numVertices) != EOF){
    int k;

    G = graphInit(numVertices);
    
    grau = malloc(numVertices*sizeof(int));
    pai = malloc(numVertices*sizeof(int));
    
    for(k = 0; k < numVertices; k++){
      scanf("%d",&grau[k]);
      int i;
      for(i = 0; i < grau[k]; i++){
	int x;
	scanf("%d",&x);
	insertEdge(G,k,x);
      }
    }
  }

  return 0;
}
